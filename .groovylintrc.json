{
    "extends": "recommended",
    "rules": {
        "CompileStatic": {
            "enabled": false
        },
        "DuplicateMapLiteral": {
            "enabled": false
        },
        "DuplicateStringLiteral": {
            "enabled": false
        },
        "GroovyLintWaiting": {
            "enabled": false
        },
        "LineLength": {
            "enabled": false
        },
        "NestedBlockDepth": {
            "enabled": false
        },
        "UnnecessaryGString": {
            "enabled": false
        },
        "UnnecessaryToString": {
            "enabled": false
        }
    }
}