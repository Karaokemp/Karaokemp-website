pipeline{
    agent any
    

    stages{
        stage("Choose Commit"){
            steps{
                 script {
                   values = ['Apple', 'Banana', 'Peach']
                   parameters = []
                   values.each {
                       echo it
                       parameters.add( [$class: 'BooleanParameterDefinition', defaultValue:
false, description: '', name: it ] )
                   }
 
                   res = input(
                       message: 'Was this successful?', parameters: parameters
                   )
                                                         print(res)
               }
 
        }
    }
    }
}