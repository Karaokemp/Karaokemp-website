pipeline{
    agent any
    stages{
        stage("Choose Commit"){
            steps{
                echo input([message: "Commit:",parameters:[$class: 'TextParameterDefinition', defaultValue: "${GIT_COMMIT}", description: 'choose commit to deploy', name:"deployCommit"])
            }
        }
        stage("Show Comit"){
            steps{
                echo "bla..."
            }
        }
    }